{
	"name": "kirby-link-field",
	"version": "2.0.1",
	"main": "index.js",
	"scripts": {
		"dev": "kirbyup app/index.js --watch",
		"build": "kirbyup app/index.js",
		"fix": "prettier . --write",
		"start": "docker-compose up php nginx",
		"composer": "docker-compose run composer install --ignore-platform-reqs",
		"php-cs-fixer": "docker-compose run composer vendor/bin/php-cs-fixer fix",
		"phpunit": "docker-compose run phpunit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/OblikStudio/kirby-link-field.git"
	},
	"keywords": [
		"kirby",
		"link",
		"field",
		"panel",
		"url",
		"href"
	],
	"author": "Hristiyan Dodov",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/OblikStudio/kirby-link-field/issues"
	},
	"homepage": "https://github.com/OblikStudio/kirby-link-field#readme",
	"devDependencies": {
		"kirbyup": "^1.0.2",
		"prettier": "^2.7.1"
	},
	"prettier": {
		"htmlWhitespaceSensitivity": "ignore"
	}
}
